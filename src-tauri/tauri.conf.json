{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "Messenger Wrapper",
    "version": "0.1.3",
    "identifier": "com.floyare.messenger-wrapper",
    "build": {
        "beforeDevCommand": null,
        "devUrl": null,
        "beforeBuildCommand": null,
        "frontendDist": "../ui"
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [ ],
        "security": {
            "capabilities": [ "default" ],
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}
